<% form_for(@mediator) do |f| %>
  			<%= f.error_messages %>
			  <p>
			    <%= f.label :name %><br />
			    <%= f.text_field :name %>
			  </p>
			  <p>
			    <%= f.label :url %><br />
			    <%= f.text_field :url %>
			  </p>
			  <p>
			    <%= f.label :username %><br />
			    <%= f.text_field :username %>
			  </p>
			  <p>
			    <%= f.label :password %><br />
			    <%= f.password_field :password %>
			  </p>
			  <p>
			    <%= f.label :password_confirmation %><br />
			    <%= f.password_field :password_confirmation %>
			  </p>
			  <p>
			    <%= f.submit 'Save' %>
			  </p>
			<% end %>